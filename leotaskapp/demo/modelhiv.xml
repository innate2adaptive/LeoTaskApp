<?xml version="1.0" encoding="utf-8"?>
<Tasks>
  <!--[1] "Hybrid spreading mechanisms and T cell activation shape the dynamics of HIV-1 infection" -->
  <name val="modelhiv"/><usage val="1"/><dataLevels val="SA;S%T"/>
  <variables class="org.leores.task.app.ModelHIV">
    <!-- Default parameter values in Table 1 of the paper [1]-->
    <load val="
      q0=708;s0=17;l0=0;i0=0;v0=1E-6;
      a=0.01;r=0.5;ri=0.0001;b=0.17;c=330;p=1;nM=800;theta=0.56;beta1=0.19;beta2=1.35E-3;g=100;kappa=1.5;iS=0.1;
      gq=0.001;gs=#1/16#;gl=$gq$;gi=0.5;gv=3;      
      mt=2000;dt=0.2;D=30;trtFrom=-1;trtTo=-1;
      trtB1=$beta1$;trtB2=$beta2$;trtG=$g$;trtA=$a$;trtP=$p$}
    "/>
    <!-- Here are some of model calculations conducted in the paper [1]-->    
    <load val="
	  name=hiv}
	  name=hiv-no-cell-free-spread;i0=1E-6;v0=0;beta2=0}
	  name=hiv-no-cell-to-cell-spread;beta1=0}
	  name=hiv-no-cell-free-double-cell-to-cell;i0=1E-6;v0=0;beta2=0;beta1=#2*$beta1$#}
	  name=hiv-double-cell-free-no-cell-to-cell;beta1=0;beta2=#2*$beta2$#}
	  name=hiv-double-activation-after-D;trtFrom=$D$;trtTo=#$mt$+1#;trtA=#2*$a$#}
      name=hiv-treatment-from-day3;trtFrom=3;trtTo=#$trtFrom$+30#;trtB1=0;trtB2=0;trtG=0}
      name=hiv-treatment-from-cd4-of-500;trtFrom=1354;trtTo=#$trtFrom$+30#;trtB1=0;trtB2=0;trtG=0}
      name=hiv-treatment-from-cd4-of-350;trtFrom=1408;trtTo=#$trtFrom$+30#;trtB1=0;trtB2=0;trtG=0}
	"/>
	<smLoad val="beforeStyleVar=pi=300;}"/>
  </variables>
  <statistics>       
    <members>      
      <!--<i><info val="%plotm-@Densities-of-quiescent-susceptible-latent-infected-cells-beforeStep@"/><valVar val="q;s;l;i"/><parVars val="t"/></i>-->
	  <i><info val="%plotm-@Densities-of-CD4-and-virus-load-beforeStep@"/><valVar val="n;v"/><parVars val="t"/></i>
    </members>
  </statistics>
</Tasks>
